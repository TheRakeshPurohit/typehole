{
  "name": "typehole",
  "displayName": "Typehole",
  "publisher": "rikurouvila",
  "description": "ðŸ§ª  Take samples of runtime values and turn them into type definitions automatically",
  "repository": "https://github.com/rikukissa/typehole",
  "version": "0.1.3",
  "icon": "images/logo.png",
  "galleryBanner": {
    "color": "#222145",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Debuggers",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "Typehole: Remove all typeholes from all files",
        "command": "typehole.remove-from-all-files"
      },
      {
        "title": "Typehole: Remove all typeholes from current file",
        "command": "typehole.remove-from-current-file"
      },
      {
        "title": "Typehole: Stop server",
        "command": "typehole.stop-server"
      },
      {
        "title": "Typehole: Start server",
        "command": "typehole.start-server"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "test": "jest"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.22",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.5",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@phenomnomnominal/tsquery": "^4.1.1",
    "@types/esquery": "^1.0.1",
    "@types/npm": "^2.0.31",
    "esquery": "^1.4.0",
    "fastify": "^3.14.2",
    "fastify-cors": "^5.2.0",
    "lmify": "^0.3.0",
    "npm": "^7.10.0",
    "typescript": "^4.1.3"
  }
}
